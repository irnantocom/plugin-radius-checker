{
    "name": "stepandalecky/xml-element",
    "type": "library",
    "description": "A better XML element that is easy to iterate on and keeps XML structure under control.",
    "keywords": ["xml", "element", "parse"],
    "license": "MIT",
    "authors": [
        {
            "name": "Stepan Dalecky",
            "homepage": "http://www.stepandalecky.cz",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "incoming+stepandalecky/xml-element@gitlab.com",
        "issues": "https://gitlab.com/stepandalecky/xml-element/issues"
    },
    "require": {
        "php": ">=7.0",
        "ext-simplexml": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "scripts": {
        "units": "phpunit tests/StepanDaleckyTests",
        "check": [
            "@units"
        ]
    },
    "autoload": {
        "psr-4": {
            "": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "StepanDaleckyTests\\": "tests/StepanDaleckyTests/"
        }
    }
}
